package views

templ MakeNavBar() {
    <header>
        <nav>
            <ul>
                for _, link := range GetUrchinLinks() {
                    <li><a href={templ.URL(link.Href)}>{ link.Name }</a></li> // Pass the links in here
                }
            </ul>
        </nav>
    </header>
    <br />
}
